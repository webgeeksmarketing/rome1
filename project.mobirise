          "_name": "about",
          "_customHTML": "<section class=\"about1\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n            <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n            <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <header>Image</header>\n            <input type=\"range\" inline=\"\" title=\"Image Height\" name=\"imgHeight\" min=\"300\" max=\"700\" step=\"50\" value=\"350\">\n            <input type=\"checkbox\" title=\"Image Effect\" name=\"imgEffect\">\n            <input type=\"color\" title=\"Image Overlay Color\" name=\"imgOverlayColor1\" value=\"#fa584d\" condition=\"imgEffect\">\n            <input type=\"range\" inline=\"\" title=\"Image Overlay Opacity\" name=\"imgOverlayOpacity\" min=\"0\" max=\"0.9\" step=\"0.05\" value=\"0.4\" condition=\"imgEffect\">\n        <header>Text</header>\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Button\" name=\"showButtons\">\n         <header>Background</header>   \n        <input type=\"color\" title=\"Background Color\" value=\"#e3e3e3\" name=\"bgColor\">\n    </mbr-parameters>\n\n   <div class=\"container\">\n        <div class=\"mbr-row wrapper rounded__10\">\n            <div class=\"mbr-col-sm-12 mbr-col-md-6 mbr-col-lg-6 mbr-px-0\">\n                <div class=\"image\">\n                    <div class=\"mbr-overlay rounded__10\"></div>\n                    <amp-img src=\"@PROJECT_PATH@/assets/images/grey-background-506x325.jpg\" alt=\"image\" height=\"350\" width=\"544.9230769230769\" layout=\"responsive\" class=\"mobirise-loader rounded__10\">\n                        <div