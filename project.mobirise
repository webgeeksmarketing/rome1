              "padding": "0 35px 38px"
            },
            "& when (@showCardsTitle)": {
              ".card__title": {
                "margin-top": "26px"
              }
            },
            "& when (@showCardsText)": {
              ".divider": {
                "width": "30px",
                "background-color": "contrast(@cardBG)",
                "opacity": ".3"
              }
            },
            "& when (@showCardsButtons)": {
