        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 10px)",
            "padding-bottom": "(@paddingBottom * 10px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              "> .mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            "& when (@paddingTop < 3), (@paddingBottom < 3)": {
              "overflow": "visible"
            },
            ".card": {
              "margin-bottom": "30px",
              "& when not (@transparentCard)": {
                ".inner-item": {
                  "background-color": "@cardBG",
                  "position": "relative",
                  "height": "100%",
                  "box-shadow": "0px 0px 35px 0px rgba(0, 0, 0, 0.05)"
                }
              },
              "& when (@showImg)": {
                ".image": {
                  "height": "(@imgHeight * 1px)",
                  "min-height": "200px",
                  "width": "100%",
                  "position": "relative",
                  "overflow": "hidden",
                  "amp-img, img": {
                    "& when (@imgEffect)": {
                      "filter": "grayscale(100%)"
                    },
                    "& when not (@transparentCard)": {
                      "border-bottom-left-radius": "0",
                      "border-bottom-right-radius": "0"
                    }
                  }
                },
                "& when (@imgEffect)": {
                  ".mbr-overlay": {
                    "opacity": "@imgOverlayOpacity",
                    "z-index": "1",
                    "pointer-events": "none",
                    "& when not (@transparentCard)": {
                      "border-bottom-left-radius": "0",
                      "border-bottom-right-radius": "0"
                    }
                  },
                  ".item1 .mbr-overlay": {
                    "background-color": "@imgOverlayColor1"
                  },
                  ".item2 .mbr-overlay": {
                    "background-color": "@imgOverlayColor2"
                  },
                  ".item3 .mbr-overlay": {
                    "background-color": "@imgOverlayColor3"
                  },
                  ".item4 .mbr-overlay": {
                    "background-color": "@imgOverlayColor4"
                  }
                }
              }
            },
            "& when (@showImgIcon) and not (@transparentCard)": {
              ".icon__image": {
                "bottom": "-1px",
                "width": "(@IconSize * 2px)",
                "height": "(@IconSize * 1.6px)",
                "z-index": "9",
                "position": "absolute",
                "padding": "10px 0 0",
                "background-color": "@cardBG",
                "border-top-right-radius": "7px",
                "border-top-left-radius": "7px",
                "left": "~'calc(50% - @{IconSize}px)'",
                ".mbr-iconfont": {
                  "font-size": "(@IconSize * 1px)",
                  "width": "(@IconSize * 1px)"
                }
              },
              ".itemImg1": {
                "color": "@imgOverlayColor1"
              },
              ".itemImg2": {
                "color": "@imgOverlayColor2"
              },
              ".itemImg3": {
                "color": "@imgOverlayColor3"
              },
              ".itemImg4": {
                "color": "@imgOverlayColor4"
              }
            },
            ".card-text": {
              "padding": "0 35px 38px"
            },
            "& when (@showCardsTitle)": {
              ".card__title": {
                "margin-top": "26px"
              }
            },
            "& when (@showCardsText)": {
              ".divider": {
                "width": "30px",
                "background-color": "contrast(@cardBG)",
                "opacity": ".3"
              }
            },
            "& when (@showCardsButtons)": {
              ".btn, .mbr-section-btn": {
                "margin-left": "0",
                "margin-right": "0"
              },
              ".btn": {
                "margin-top": "30px",
                "width": "100%",
                ".btn__hover": {
                  "transition": "transform 0.4s ease"
                },
                ".mbr-iconfont": {
                  "position": "relative",
                  "opacity": "0",
                  "margin-top": "1px",
                  "transition": "opacity 0.15s ease, .15s width 0s",
                  "width": "0"
                },
                "&:hover": {
                  ".mbr-iconfont": {
                    "opacity": "1",
                    "width": "15px"
                  },
                  ".btn__hover": {
                    "transform": "translateX(-10px)"
                  }
                }
              }
            },
            ".card__text": {
              "text-align": "center"
            }
          },
          "_name": "features2",
          "_customHTML": "<section class=\"features2\" group=\"Features\" plugins=\"amp-main\">\n    <mbr-parameters>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n            <header>Cards</header>\n        <select title=\"CardsAmount\" name=\"cardsAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected=\"\">4</option>\n        </select>\n        <input type=\"checkbox\" title=\"Transparent Card\" name=\"transparentCard\">\n        <input type=\"color\" title=\"Card Background\" name=\"cardBG\" value=\"#ffffff\" condition=\"!transparentCard\">\n        <header>Cards Text</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showCardsTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showCardsText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Buttons\" name=\"showCardsButtons\" checked=\"\">\n        <header>Cards Image</header>\n        <input type=\"checkbox\" title=\"Show Image\" name=\"showImg\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Image Height\" name=\"imgHeight\" min=\"200\" max=\"600\" step=\"10\" value=\"360\" condition=\"showImg\">\n        <input type=\"checkbox\" title=\"Image Effect\" name=\"imgEffect\" condition=\"showImg\" checked=\"\">\n        <input type=\"color\" title=\"Color 1\" name=\"imgOverlayColor1\" value=\"#851618\" condition=\"((showImgIcon &amp;&amp; !transparentCard) || imgEffect) &amp;&amp; cardsAmount >=1 &amp;&amp; showImg\">\n        <input type=\"color\" title=\"Color 2\" name=\"imgOverlayColor2\" value=\"#031b4e\" condition=\"((showImgIcon &amp;&amp; !transparentCard) || imgEffect) &amp;&amp; cardsAmount >=2 &amp;&amp; showImg\">\n        <input type=\"color\" title=\"Color 3\" name=\"imgOverlayColor3\" value=\"#851618\" condition=\"((showImgIcon &amp;&amp; !transparentCard) || imgEffect) &amp;&amp; cardsAmount >=3 &amp;&amp; showImg\">\n        <input type=\"color\" title=\"Color 4\" name=\"imgOverlayColor4\" value=\"#031b4e\" condition=\"((showImgIcon &amp;&amp; !transparentCard) || imgEffect) &amp;&amp; cardsAmount >=4 &amp;&amp; showImg\">\n        <input type=\"range\" inline=\"\" title=\"Image Overlay Opacity\" name=\"imgOverlayOpacity\" min=\"0\" max=\"0.9\" step=\"0.05\" value=\"0.3\" condition=\"showImg &amp;&amp; imgEffect\">\n        <input type=\"checkbox\" title=\"Show Image Icon\" name=\"showImgIcon\" condition=\"showImg &amp;&amp; !transparentCard\" checked=\"\">\n        <input type=\"range\" title=\"Icon Size\" name=\"IconSize\" min=\"20\" max=\"40\" step=\"5\" value=\"30\" condition=\"showImg &amp;&amp; showImgIcon &amp;&amp; !transparentCard\">\n        <!--  -->\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" required=\"\">\n            <input type=\"color\" title=\"Background Color\" value=\"#e3e3e3\" selected=\"\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/ect24.jpg\">\n        </fieldset>\n        <!--  -->\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#3b7eff\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <div class=\"\" mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n\n        <div class=\"mbr-row\" mbr-class=\"{'mbr-jc-c' : cardsAmount<3}\">\n            <div class=\"mbr-col-sm-12 card\" mbr-class=\"{'mbr-col-lg-4' : cardsAmount<4, 'mbr-col-lg-3' : cardsAmount==4, 'mbr-col-md-6': cardsAmount ==4 || cardsAmount < 4}\">\n                <div class=\"inner-item rounded__10\">\n                    <div class=\"card-img\">\n                        \n                        <div class=\"image item1\" mbr-if=\"showImg\">\n                            <div class=\"mbr-overlay rounded__10\" mbr-if=\"imgEffect\"></div>\n                            <amp-img src=\"@PROJECT_PATH@/assets/images/grey-background-506x325.jpg\" alt=\"image\" height=\"360\" width=\"560.4923076923077\" layout=\"responsive\" class=\"mobirise-loader rounded__10\">\n                                <div placeholder=\"\" class=\"placeholder\"></div>\n                                <a href=\"#\"><img src=\"@PROJECT_PATH@/assets/images/grey-background-506x325.jpg\" class=\"mbr-temp\" alt=\"\" title=\"\"></a>\n                            </amp-img>\n                            <div class=\"align-center icon__image\" mbr-if=\"showImgIcon\"><span class=\"mbr-iconfont-btn itemImg1 mbr-iconfont fa-line-chart fa\" mbr-icon=\"\"></span></div>\n                        </div>\n                        <div class=\"card-text align-center\">\n                            <div class=\"card-text-box\">\n                               <a href=\"#\"> <h3 mbr-if=\"showCardsTitle\" class=\"mbr-fonts-style card__title mbr-semibold mbr-black\" mbr-theme-style=\"display-6\" data-app-selector=\".card__title\">Account Executive</h3></a>\n                                <span class=\"divider\"></span>\n                                <p class=\"mbr-fonts-style mbr-regular card__text\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsText\" data-app-selector=\".card__text\">\n                                    Motivated and determined professionals are guaranteed success with an uncapped earning potential. Generate your own book of business by prospecting new clients and building relationships to grow your network. </p>\n                            </div>\n                             <div class=\"card-btn mbr-section-btn\" mbr-buttons=\"\" data-toolbar=\"-mbrBtnMove\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsButtons\"><a class=\"btn btn-sm btn-success-outline\" href=\"#\" data-app-placeholder=\"Type Text\">Apply Now!</a></div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"mbr-col-sm-12 card\" mbr-if=\"cardsAmount>1\" mbr-class=\"{'mbr-col-lg-4' : cardsAmount<4, 'mbr-col-lg-3' : cardsAmount==4, 'mbr-col-md-6': cardsAmount ==4 || cardsAmount < 4}\">\n                <div class=\"inner-item rounded__10\">\n                    <div class=\"card-img\">\n                        <div class=\"image item2\" mbr-if=\"showImg\">\n                             <div class=\"mbr-overlay rounded__10\" mbr-if=\"imgEffect\"></div>\n                            <amp-img src=\"@PROJECT_PATH@/assets/images/grey-background-506x325.jpg\" alt=\"image\" height=\"360\" width=\"560.4923076923077\" layout=\"responsive\" class=\"mobirise-loader rounded__10\">\n                                <div placeholder=\"\" class=\"placeholder\"></div>\n                                <a href=\"#\"><img src=\"@PROJECT_PATH@/assets/images/grey-background-506x325.jpg\" class=\"mbr-temp\" alt=\"\" title=\"\"></a>\n                            </amp-img>\n                            <div class=\"align-center icon__image\" mbr-if=\"showImgIcon\"><span class=\"mbr-iconfont-btn itemImg2 mbr-iconfont fa-suitcase fa\" mbr-icon=\"\"></span></div>\n                        </div>\n                        <div class=\"card-text align-center\">\n                            <div class=\"card-text-box\">\n                                <a href=\"#\"><h3 mbr-if=\"showCardsTitle\" class=\"mbr-fonts-style card__title mbr-semibold mbr-black\" mbr-theme-style=\"display-6\" data-app-selector=\".card__title\">Logistics Coordinator</h3></a>\n                                <span class=\"divider\"></span>\n                                <p class=\"mbr-fonts-style mbr-regular card__text\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsText\" data-app-selector=\".card__text\">\n                                    Do you enjoy putting the pieces of a puzzle together? As a Logistics Coordinator, you will be responsible for negotiating rates, coordinating freight and building relationships with our carrier partners. </p>\n                            </div>\n                             <div class=\"card-btn mbr-section-btn\" mbr-buttons=\"\" data-toolbar=\"-mbrBtnMove\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsButtons\"><a class=\"btn btn-sm btn-success-outline\" href=\"#\" data-app-placeholder=\"Type Text\"><span class=\"mbri-right mbr-white mbr-iconfont mbr-iconfont-btn\"></span><span class=\"btn__hover\">Apply Now!</span></a></div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"mbr-col-sm-12 card\" mbr-if=\"cardsAmount>2\" mbr-class=\"{'mbr-col-lg-4' : cardsAmount<4, 'mbr-col-lg-3' : cardsAmount==4, 'mbr-col-md-6': cardsAmount ==4 || cardsAmount < 4}\">\n                <div class=\"inner-item rounded__10\">\n                    <div class=\"card-img\">\n                        <div class=\"image item3\" mbr-if=\"showImg\">\n                             <div class=\"mbr-overlay rounded__10\" mbr-if=\"imgEffect\"></div>\n                            <amp-img src=\"@PROJECT_PATH@/assets/images/grey-background-506x325.jpg\" alt=\"image\" height=\"360\" width=\"560.4923076923077\" layout=\"responsive\" class=\"mobirise-loader rounded__10\">\n                                <div placeholder=\"\" class=\"placeholder\"></div>\n                                <a href=\"#\"><img src=\"@PROJECT_PATH@/assets/images/grey-background-506x325.jpg\" class=\"mbr-temp\" alt=\"\" title=\"\"></a>\n                            </amp-img>\n                            <div class=\"align-center icon__image\" mbr-if=\"showImgIcon\"><span class=\"mbr-iconfont-btn itemImg3 mbr-iconfont fa-user fa\" mbr-icon=\"\"></span></div>\n                        </div>\n                        <div class=\"card-text align-center\">\n                            <div class=\"card-text-box\">\n                               <a href=\"#\"> <h3 mbr-if=\"showCardsTitle\" class=\"mbr-fonts-style card__title mbr-semibold mbr-black\" mbr-theme-style=\"display-6\" data-app-selector=\".card__title\">Account Manager</h3></a>\n                                <span class=\"divider\"></span>\n                                <p class=\"mbr-fonts-style mbr-regular card__text\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsText\" data-app-selector=\".card__text\">\n                                    By maintaining strong relationships with our clients, we keep them up-to-date on their shipments. Being part of a glowing Customer Service team allows you to provide exceptional, high-quality service for our clients. </p>\n                            </div>\n                             <div class=\"card-btn mbr-section-btn\" mbr-buttons=\"\" data-toolbar=\"-mbrBtnMove\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsButtons\"><a class=\"btn btn-sm btn-success-outline\" href=\"#\" data-app-placeholder=\"Type Text\"><span class=\"mbri-right mbr-white mbr-iconfont mbr-iconfont-btn\"></span><span class=\"btn__hover\">Apply Now!</span></a></div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n                        <div class=\" mbr-col-sm-12 card\" mbr-if=\"cardsAmount>3\" mbr-class=\"{'mbr-col-lg-4' : cardsAmount<4, 'mbr-col-lg-3' : cardsAmount==4, 'mbr-col-md-6': cardsAmount ==4 || cardsAmount < 4}\">\n                <div class=\"inner-item rounded__10\">\n                    <div class=\"card-img\">\n                        <div class=\"image item4\" mbr-if=\"showImg\">\n                             <div class=\"mbr-overlay rounded__10\" mbr-if=\"imgEffect\"></div>\n                            <amp-img src=\"@PROJECT_PATH@/assets/images/grey-background-506x325.jpg\" alt=\"image\" height=\"360\" width=\"560.4923076923077\" layout=\"responsive\" class=\"mobirise-loader rounded__10\">\n                                <div placeholder=\"\" class=\"placeholder\"></div>\n                                <a href=\"#\"><img src=\"@PROJECT_PATH@/assets/images/grey-background-506x325.jpg\" class=\"mbr-temp\" alt=\"\" title=\"\"></a>\n                            </amp-img>\n                            <div class=\"align-center icon__image\" mbr-if=\"showImgIcon\"><span class=\"mbr-iconfont-btn itemImg4 mbr-iconfont fa-bar-chart-o fa\" mbr-icon=\"\"></span></div>\n                        </div>\n                        <div class=\"card-text align-center\">\n                            <div class=\"card-text-box\">\n                               <a href=\"#\"><h3 mbr-if=\"showCardsTitle\" class=\"mbr-fonts-style card__title mbr-semibold mbr-black\" mbr-theme-style=\"display-6\" data-app-selector=\".card__title\">Administration<br>Department</h3></a>\n                                <span class=\"divider\"></span>\n                                <p class=\"mbr-fonts-style mbr-regular card__text\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsText\" data-app-selector=\".card__text\">\n                                    Our administration teams offer opportunities in Human Resources, Accounts Payable and Accounts Receivable. There are also specialist positions in our Claims and Account departments. </p>\n                            </div>\n                             <div class=\"card-btn mbr-section-btn\" mbr-buttons=\"\" data-toolbar=\"-mbrBtnMove\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsButtons\"><a class=\"btn btn-sm btn-success-outline\" href=\"#\" data-app-placeholder=\"Type Text\"><span class=\"mbri-right mbr-iconfont mbr-white mbr-iconfont-btn\"></span><span class=\"btn__hover\">Apply Now!</span></a></div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
