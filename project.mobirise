input type=\"checkbox\" title=\"Copyright\" name=\"showCopyright\" checked=\"\">\n    <!--  -->\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/bg5.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#000000\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked=\"\" condition=\"bg.type !== 'color'\">\n    <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n    <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n    </mbr-parameters>\n\n<div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <div class=\"container\">\n        <div class=\"mbr-row footer-wrap mbr-jc-c\">\n            <div class=\"image-block align-left mbr-col-sm-12 mbr-col-lg-2\" mbr-class=\"{'mbr-col-md-4' : columnsCount == 2, 'mbr-col-md-3' : columnsCount == 3, 'mbr-col-md-6' : columnsCount <= 1}\" mbr-if=\"showLogo\">\n                    <amp-img src=\"@PROJECT_PATH@/assets/images/rome-logo-transparent-white-260x117.png\" layout=\"responsive\" width=\"128.88888888888889\" height=\"58\" alt=\"Mobirise\" class=\"mobirise-loader\">\n                        <div placeholder=\"\" class=\"placeholder\"></div>\n                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/rome-logo-transparent-white-260x117.png\" alt=\"Mobirise\" title=\"\">\n                    </amp-img>\n            </div>\n\n            <div class=\"items-col mbr-col-sm-12 align-left\" mbr-class=\"{'mbr-col-lg-2': columnsCount == 3, 'mbr-col-lg-3': ((columnsCount <= 2) &amp;&amp; (showLogo)) || (columnsCount == 2), 'mbr-col-md-4' : columnsCount == 2, 'mbr-col-md-3' : columnsCount == 3, 'mbr-col-md-6' : columnsCount <= 1, 'mbr-col-lg-6 mbr-col-md-6 align-center' : ((columnsCount < 2) &amp;&amp; (!showLogo)) }\">\n                <h3 class=\"mbr-fonts-style mbr-white group-title mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".group-title\">USEFUL LINKS</h3>\n                <div class=\"items mbr-white\" mbr-list=\"\" mbr-list-grow=\"columnItems1\">\n                    <p class=\"item mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item, .items\">\n                        About Us</p>\n                    \n                    \n                <p class=\"item mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item, .items\">\n                        Our Services\n                    </p><p class=\"item mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item, .items\">\n                        Our Team</p></div>\n            </div>\n\n            <div class=\"items-col mbr-col-sm-12 align-left\" mbr-class=\"{'mbr-col-lg-2': columnsCount == 3, 'mbr-col-lg-3': columnsCount <= 2, 'mbr-col-md-4' : columnsCount == 2, 'mbr-col-md-3' : columnsCount == 3}\" mbr-if=\"columnsCount > 1\">\n                <h3 class=\"mbr-fonts-style mbr-white group-title mbr-bold\" mbr-theme-style=\"display-6\" mbr-if=\"showTitle\" data-app-selector=\".group-title\">OUR SERVICES</h3>\n                <div class=\"items mbr-white\" mbr-list=\"\" mbr-list-grow=\"columnItems2\">\n                    <p class=\"item mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item, .items\">Service</p>\n                    <p class=\"item mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item, .items\">\n                        Service</p>\n                    \n                <p class=\"item mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item, .items\">\n                        Service</p></div>\n            </div>\n\n 