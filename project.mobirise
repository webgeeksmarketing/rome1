            "padding-top": "(@paddingTop * 10px)",
            "padding-bottom": "(@paddingBottom * 10px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              "> .mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card": {
              "margin-bottom": "30px",
              "& when not (@transparentCard)": {
                ".inner-item": {
                  "background-color": "@cardBG",
                  "position": "relative",
                  "height": "100%",
                  "box-shadow": "0px 0px 35px 0px rgba(0, 0, 0, 0.05)"
                }
              },
              "& when (@showImg)": {
                ".image": {
                  "height": "(@imgHeight * 1px)",
                  "min-height": "200px",
                  "width": "100%",
                  "position": "relative",
                  "overflow": "hidden",
                  "amp-img, img": {
                    "& when (@imgEffect)": {
                      "filter": "grayscale(100%)"
                    },
                    "& when not (@transparentCard)": {
                      "border-bottom-left-radius": "0",
                      "border-bottom-right-radius": "0"
                    }
                  }
                },
                "& when (@imgEffect)": {
                  ".mbr-overlay": {
                    "opacity": "@imgOverlayOpacity",
                    "z-index": "1",
                    "pointer-events": "none",
                    "& when not (@transparentCard)": {
                      "border-bottom-left-radius": "0",
                      "border-bottom-right-radius": "0"
                    }
                  },
                  ".item1 .mbr-overlay": {
                    "background-color": "@imgOverlayColor1"
                  },
                  ".item2 .mbr-overlay": {
                    "background-color": "@imgOverlayColor2"
                  },
                  ".item3 .mbr-overlay": {
                    "background-color": "@imgOverlayColor3"
                  },
                  ".item4 .mbr-overlay": {
                    "background-color": "@imgOverlayColor4"
                  }
                }
              }
            },
            "& when (@showImgIcon) and not (@transparentCard)": {
              ".icon__image": {
                "bottom": "-1px",
                "width": "(@IconSize * 2px)",
                "height": "(@IconSize * 1.6px)",
                "z-index": "9",
                "position": "absolute",
                "padding": "10px 0 0",
                "background-color": "@cardBG",
                "border-top-right-radius": "7px",
                "border-top-left-radius": "7px",
                "left": "~'calc(50% - @{IconSize}px)'",
                ".mbr-iconfont": {
                  "font-size": "(@IconSize * 1px)",
                  "width": "(@IconSize * 1px)"
                }
              },
              ".itemImg1": {
                "color": "@imgOverlayColor1"
              },
              ".itemImg2": {
                "color": "@imgOverlayColor2"
              },
              ".itemImg3": {
                "color": "@imgOverlayColor3"
              },
              ".itemImg4": {
                "color": "@imgOverlayColor4"
              }
            },
            ".card-text": {
              "padding": "0 35px 38px"
            },
            "& when (@showCardsTitle)": {
              ".card__title": {
                "margin-top": "26px"
              }
            },
            "& when (@showCardsText)": {
              ".divider": {
                "width": "30px",
                "background-color": "contrast(@cardBG)",
                "opacity": ".3"
              }
            },
            "& when (@showCardsButtons)": {
              ".btn, .mbr-section-btn": {
                "margin-left": "0",
                "margin-right": "0"
              },
              ".btn": {
                "margin-top": "30px",
                "width": "100%",
                ".btn__hover": {
                  "transition": "transform 0.4s ease"
                },
                ".mbr-iconfont": {
                  "position": "relative",
                  "opacity": "0",
                  "margin-top": "1px",
                  "transition": "opacity 0.15s ease, .15s width 0s",
                  "width": "0"
                },
                "&:hover": {
                  ".mbr-iconfont": {
                    "opacity": "1",
                    "width": "15px"
                  },
                  ".btn__hover": {
                    "transform": "translateX(-10px)"
                  }
                }
              }
            },
            ".card__title": {
              "color": "#851618"
            },
            ".card__text": {
              "color": "#000000"
            }
