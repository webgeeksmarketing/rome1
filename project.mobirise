          "_name": "features9",
          "_customHTML": "<section class=\"features9\" group=\"Features\" plugins=\"amp-main\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"10\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" title=\"Show Items\" name=\"showItems\" min=\"1\" max=\"12\" step=\"1\" value=\"6\">\n        <input type=\"range\" inline title=\"Column Count\" name=\"cardsCount\" min=\"1\" max=\"4\" step=\"1\" value=\"3\" condition=\"showCardsTitle  || showCardsText\">\n        <header>Cards</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showCardsTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showCardsText\" checked>\n        <select title=\"Align\" name=\"cardsAlign\" condition=\"showCardsText\">\n            <option value=\"1\" selected>Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\" checked>\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#ffffff\" condition=\"(showCardsTitle  || showCardsText) && !transparentBg\">\n        <input type=\"range\" title=\"Opacity\" name=\"bgOpacity\" min=\"0\" max=\"1\" value=\"1\" step=\"0.1\" condition=\"!transparentBg\">\n        <input type=\"color\" title=\"Color Icon\" name=\"IconColor\" value=\"#2e2e2e\">\n        <input type=\"range\" inline title=\"Size Icon\" name=\"iconSize\" min=\"20\" max=\"40\" step=\"2\" value=\"30\">\n        <input type=\"color\" title=\"Background Icon Color\" name=\"bgIcon\" value=\"#eef3f9\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" required>\n            <input type=\"color\" title=\"Background Color\" value=\"#e3e3e3\" selected>\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/bg7.jpg\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#d8d8d8\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"mbr-row\" mbr-class=\"{'mbr-jc-c': cardsCount <= 3}\" mbr-list=\"\" mbr-list-grow=\"showItems\">\n            <div class=\"card mbr-col-sm-12\" mbr-class=\"{\n                             'mbr-col-md-12': cardsCount == 1,\n                             'mbr-col-md-6 mbr-col-lg-6': cardsCount == 2,\n                             'mbr-col-md-8 mbr-col-lg-4': cardsCount == 3,\n                             'mbr-col-md-6 mbr-col-lg-3': cardsCount == 4,\n                             'last-child': cardsCount == 1}\">\n                <div class=\"card-wrapper mbr-flex align-left\" mbr-class=\"{'align-center': cardsAlign == 2, 'align-right': cardsAlign == 3}\">\n                    <div class=\"card-img\">\n                        <div class=\"align-center mbr-flex mbr-jc-c icon__item rounded__7\">\n                        <span class=\"mbr-iconfont mbr-flex mbr-align-center mbr-jc-c mbr-iconfont-btn fa-phone-square fa\" mbr-icon></span>\n                        </div>\n                    </div>\n                    <div class=\"card-box\">\n                        <h4 class=\"card-subtitle mbr-fonts-style mbr-semibold mbr-black\" mbr-theme-style=\"display-6\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-subtitle\">Call us now!\n</h4>\n                        <p class=\"card-text mbr-text mbr-mt-1 mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsText\" data-app-selector=\".card-text\">Toll Free: 866-835-7663 \n<br>Local: 519-489-2660</p>\n                    \n                    </div>\n                </div>\n            </div>\n            \n            \n            \n        <div class=\"card mbr-col-sm-12\" mbr-class=\"{\n                             'mbr-col-md-12': cardsCount == 1,\n                             'mbr-col-md-6 mbr-col-lg-6': cardsCount == 2,\n                             'mbr-col-md-8 mbr-col-lg-4': cardsCount == 3,\n                             'mbr-col-md-6 mbr-col-lg-3': cardsCount == 4,\n                             'last-child': cardsCount == 1}\">\n                <div class=\"card-wrapper mbr-flex align-left\" mbr-class=\"{'align-center': cardsAlign == 2, 'align-right': cardsAlign == 3}\">\n                    <div class=\"card-img\">\n                        <div class=\"align-center mbr-flex mbr-jc-c icon__item rounded__7\">\n                        <span class=\"mbr-iconfont mbr-flex mbr-align-center mbr-jc-c mbr-iconfont-btn fa-home fa\" mbr-icon></span>\n                        </div>\n                    </div>\n                    <div class=\"card-box\">\n                        <h4 class=\"card-subtitle mbr-fonts-style mbr-semibold mbr-black\" mbr-theme-style=\"display-6\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-subtitle\">Administration</h4>\n                        <p class=\"card-text mbr-text mbr-mt-1 mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsText\" data-app-selector=\".card-text\">Toll Free: 800-869-1854&nbsp;<br>Local: 519-883-8360\n<br><br></p>\n                    \n                    </div>\n                </div>\n            </div><div class=\"card mbr-col-sm-12\" mbr-class=\"{\n                             'mbr-col-md-12': cardsCount == 1,\n                             'mbr-col-md-6 mbr-col-lg-6': cardsCount == 2,\n                             'mbr-col-md-8 mbr-col-lg-4': cardsCount == 3,\n                             'mbr-col-md-6 mbr-col-lg-3': cardsCount == 4,\n                             'last-child': cardsCount == 1}\">\n                <div class=\"card-wrapper mbr-flex align-left\" mbr-class=\"{'align-center': cardsAlign == 2, 'align-right': cardsAlign == 3}\">\n                    <div class=\"card-img\">\n                        <div class=\"align-center mbr-flex mbr-jc-c icon__item rounded__7\">\n                        <span class=\"mbr-iconfont mbr-flex mbr-align-center mbr-jc-c mbr-iconfont-btn fa-share-square-o fa\" mbr-icon></span>\n                        </div>\n                    </div>\n                    <div class=\"card-box\">\n                        <h4 class=\"card-subtitle mbr-fonts-style mbr-semibold mbr-black\" mbr-theme-style=\"display-6\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-subtitle\">Fax 1</h4>\n                        <p class=\"card-text mbr-text mbr-mt-1 mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsText\" data-app-selector=\".card-text\">Toll Free: 866-329-7663&nbsp;<br>Local: 519-885-965</p>\n                    \n                    </div>\n                </div>\n            </div><div class=\"card mbr-col-sm-12\" mbr-class=\"{\n                             'mbr-col-md-12': cardsCount == 1,\n                             'mbr-col-md-6 mbr-col-lg-6': cardsCount == 2,\n                             'mbr-col-md-8 mbr-col-lg-4': cardsCount == 3,\n                             'mbr-col-md-6 mbr-col-lg-3': cardsCount == 4,\n                             'last-child': cardsCount == 1}\">\n                <div class=\"card-wrapper mbr-flex align-left\" mbr-class=\"{'align-center': cardsAlign == 2, 'align-right': cardsAlign == 3}\">\n                    <div class=\"card-img\">\n                        <div class=\"align-center mbr-flex mbr-jc-c icon__item rounded__7\">\n                        <span class=\"mbr-iconfont mbr-flex mbr-align-center mbr-jc-c mbr-iconfont-btn fa-location-arrow fa\" mbr-icon></span>\n                        </div>\n                    </div>\n                    <div class=\"card-box\">\n                        <h4 class=\"card-subtitle mbr-fonts-style mbr-semibold mbr-black\" mbr-theme-style=\"display-6\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-subtitle\">Corporate Head Office</h4>\n                        <p class=\"card-text mbr-text mbr-mt-1 mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsText\" data-app-selector=\".card-text\">100 Campbell Ave., Unit 2\n<br>Kitchener, Ontario\n<br>Canada N2H 4X8</p>\n                    \n                    </div>\n                </div>\n            </div><div class=\"card mbr-col-sm-12\" mbr-class=\"{\n                             'mbr-col-md-12': cardsCount == 1,\n                             'mbr-col-md-6 mbr-col-lg-6': cardsCount == 2,\n                             'mbr-col-md-8 mbr-col-lg-4': cardsCount == 3,\n                             'mbr-col-md-6 mbr-col-lg-3': cardsCount == 4,\n                             'last-child': cardsCount == 1}\">\n                <div class=\"card-wrapper mbr-flex align-left\" mbr-class=\"{'align-center': cardsAlign == 2, 'align-right': cardsAlign == 3}\">\n                    <div class=\"card-img\">\n                        <div class=\"align-center mbr-flex mbr-jc-c icon__item rounded__7\">\n                        <span class=\"mbr-iconfont mbr-flex mbr-align-center mbr-jc-c mbr-iconfont-btn fa-mail-reply-all fa\" mbr-icon></span>\n                        </div>\n                    </div>\n                    <div class=\"card-box\">\n                        <h4 class=\"card-subtitle mbr-fonts-style mbr-semibold mbr-black\" mbr-theme-style=\"display-6\" mbr-if=\"showCardsTitle\" data-app-selector=\"